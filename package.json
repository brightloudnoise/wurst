{
  "name": "wurst",
  "description": "directory based autoloader for hapi.js routes",
  "keywords": [
    "hapi",
    "api",
    "restful",
    "rest",
    "routes",
    "autoloader",
    "auto",
    "loader"
  ],
  "version": "0.1.2",
  "license": "MIT",
  "author": {
    "name": "Felix Heck",
    "email": "hi@whoTheHeck.de",
    "url": "http://whoTheHeck.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/felixheck/wurst.git"
  },
  "bugs": {
    "email": "hi@whoTheHeck.com",
    "url": "https://github.com/felixheck/wurst/issues"
  },
  "main": "./lib/index.js",
  "config": {
    "mochaOptions": "test/** --colors --reporter min --compilers js:babel-core/register"
  },
  "scripts": {
    "build": "babel -d lib src",
    "coverage": "NODE_ENV=test istanbul cover _mocha -- $npm_package_config_mochaOptions",
    "lint": "eslint --ext .js src test",
    "start": "npm run test -- --watch",
    "test": "NODE_ENV=test mocha $npm_package_config_mochaOptions",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "prebuild": "rm -rf lib",
    "precoverage": "rm -rf coverage",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "2.9.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "hapi": "^13.4.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5"
  },
  "engines": {
    "node": "^6.0.0",
    "npm": "^3.8.8"
  },
  "dependencies": {
    "glob": "^7.0.3",
    "joi": "^8.1.0"
  },
  "peerDependencies": {
    "hapi": "13.x.x"
  }
}
